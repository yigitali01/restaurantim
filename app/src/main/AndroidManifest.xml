<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.restorant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OrderDetails"></activity>
        <activity android:name=".TableDisplay" />
        <activity android:name=".EditProfile" />
        <activity android:name=".StockActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".LogIn" />
        <activity android:name=".AddProduct" />
        <activity android:name=".SignUp" />
        <activity android:name=".AddTable" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>